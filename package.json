{
    "name": "django-gettext",
    "displayName": "django.po",
    "description": "language support for django gettext",
    "version": "0.0.8",
    "engines": {
        "vscode": "^1.73.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "keywords": [
        "django",
        "django.po",
        "gettext",
        "po"
    ],
    "publisher": "xiangcai",
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "makemessages",
                "title": "Django makemessages"
            }
        ],
        "languages": [
            {
                "id": "django.po",
                "aliases": [
                    "Django Gettext",
                    "django.po"
                ],
                "extensions": [
                    ".po"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "light": "./images/icon.svg",
                    "dark": "./images/icon.svg"
                }
            }
        ],
        "grammars": [
            {
                "language": "django.po",
                "scopeName": "django.po",
                "path": "./syntaxes/django.po.tmLanguage.json",
                "embeddedLanguages": {
                    "string.quoted.double.django.po": "python"
                }
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/runningzyp/django-gettext.git"
    },
    "license": "MIT",
    "activationEvents": [
        "onCommand:makemessages",
        "onLanguage:django.po"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "@vscode/test-electron": "^2.2.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.26.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.8.4"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
